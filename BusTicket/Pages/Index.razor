@code {
    int cinsiyet;
   
    int? selectedSeatNumber;

    private void UpdateSelectedSeatNumber(int seatNumber)
    {
        selectedSeatNumber = seatNumber;
    }

    IEnumerable<Yolcu>? yolcus;
    Yolcu? yolcu = new Yolcu()
    {
            DogumTarihi = DateTime.Parse("16/07/2023"),
    };

    string? ad;
    string? soyad;

    void Submit(Yolcu arg)
    {
        NotificationMessage notificationMessage = new NotificationMessage() { Severity = NotificationSeverity.Success, Summary = selectedSeatNumber.ToString(), Detail = "Biletiniz reserve edilmiştir.", Duration = 4000 };
        NotificationService.Notify(notificationMessage);
    }

    void Cancel()
    {
        //
    }

}