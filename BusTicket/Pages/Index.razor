@page "/"
@using BusTicket.Models;
@using Microsoft.AspNetCore.Components;
@using BusTicket.Component;
@inject NotificationService NotificationService
<PageTitle>Index</PageTitle>


<RadzenRow class="rz-mt-12" style="background-color:#fafbff;border-radius: 12px;">
    <RadzenTemplateForm Data="@yolcu" Submit="@((Yolcu args) => { Submit(args); })">
        <RadzenRow Gap="2rem" Class="rz-p-0 rz-p-lg-4" Style="width:1200px;">
            <RadzenColumn Size="12" SizeMD="4">
                <RadzenStack>
                    <RadzenFieldset Text="Yolcu Bilgisi">
                        <RadzenStack Gap="1rem">
                            <RadzenRow AlignItems="AlignItems.Center">
                                <RadzenColumn Size="12" SizeMD="4">
                                    <RadzenLabel Text="Ad" />
                                </RadzenColumn>
                                <RadzenColumn Size="12" SizeMD="8">
                                    <RadzenTextBox Style="width: 100%;" Name="Ad" @bind-Value="yolcu.Ad" />
                                </RadzenColumn>
                            </RadzenRow>

                            <RadzenRow AlignItems="AlignItems.Center">
                                <RadzenColumn Size="12" SizeMD="4">
                                    <RadzenLabel Text="Soyad" />
                                </RadzenColumn>
                                <RadzenColumn Size="12" SizeMD="8">
                                    <RadzenTextBox Style="width: 100%;" Name="Soyad" @bind-Value="yolcu.Soyad" />
                                </RadzenColumn>
                            </RadzenRow>


                            <RadzenRow AlignItems="AlignItems.Center">
                                <RadzenColumn Size="12" SizeMD="4">
                                    <RadzenLabel Text="Doğum Tarihi" />
                                </RadzenColumn>
                                <RadzenColumn Size="12" SizeMD="8">
                                    <RadzenDatePicker Style="width: 100%;" Name="ExpiryDate" @bind-Value="yolcu.DogumTarihi" DateFormat="dd.MM.yyyy" />
                                </RadzenColumn>
                            </RadzenRow>


                            <RadzenRow AlignItems="AlignItems.Center">
                                <RadzenColumn Size="12" SizeMD="4">
                                    <RadzenLabel Text="Cinsiyet" />
                                </RadzenColumn>
                                <RadzenColumn Size="12" SizeMD="8">
                                    <RadzenRadioButtonList @bind-Value=@cinsiyet TValue="int">
                                        <Items>
                                            <RadzenRadioButtonListItem Text="Erkek" Value="1" />
                                            <RadzenRadioButtonListItem Text="Kadın" Value="2" />
                                           
                                        </Items>
                                    </RadzenRadioButtonList>
                                </RadzenColumn>
                            </RadzenRow>

                        </RadzenStack>


                    </RadzenFieldset>

